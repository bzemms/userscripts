{
  "name": "GitHub-Diff-Whitespace",
  "version": "1.2",
  "description": "",
  "manifest_version": 2,
  "content_scripts": [ {
    "exclude_globs": [  ],
    "exclude_matches": [  ],
    "include_globs": [  ],
    "js": [ "github-diff-whitespace.js" ],
    "matches": [
      "https://github.com/*/commit/*",
